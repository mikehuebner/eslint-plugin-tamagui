{
  "name": "eslint-plugin-tamagui",
  "version": "0.0.1",
  "description": "An ESLint plugin for Tamagui, a universal React UI library for web and native.",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "tamagui",
    "react-native",
    "react-native-web"
  ],
  "author": "Mike Huebner (mikehuebner@me.com)",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mikehuebner/eslint-plugin-tamagui"
  },
  "scripts": {
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts,.tsx ./src",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check ./src",
    "fix:prettier": "prettier --write ./src",
    "build": "rm -rf ./dist && swc ./src -d dist",
    "test": "vitest run src/__tests__",
    "test:watch": "watchlist src -- pnpm run test",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.7.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.88",
    "@tamagui/core": "1.71.0",
    "@tamagui/helpers": "1.71.0",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@typescript-eslint/rule-tester": "^6.7.2",
    "@typescript-eslint/types": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.5",
    "watchlist": "^0.3.1"
  },
  "peerDependencies": {
    "@tamagui/core": "*",
    "@tamagui/helpers": "*",
    "eslint": ">=7"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0",
    "pnpm": ">=8.6.6"
  }
}
